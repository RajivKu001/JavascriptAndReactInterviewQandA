let arr = [4, 5, 6, 7, 0, 3, 2, 8, 9, 10];

function findUnsortedElements(arr) {
    let unsortedElements = [];

    for (let i = 0; i < arr.length - 1; i++) {
        if (arr[i] > arr[i + 1]) {
            unsortedElements.push(arr[i]);
        }
    }

    return unsortedElements;
}

console.log(findUnsortedElements(arr)); // Output: [0, 3, 2]
