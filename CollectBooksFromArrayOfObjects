  let books = [
    {
      name: "Anaya",
      books: ["Bible", "harry poter"],
      age: 21
    },
    {
      name: "Annna",
      books: ["Neta", "Das g"],
      age: 20
    },
    {
      name: "Amma",
      books: ["Bible", "harry poter"],
      age: 25
    }
  ];

  const book = books.reduce((acc, curr) => {
    return [...acc, ...curr.books];
  }, []);

  console.log(book.flat());

