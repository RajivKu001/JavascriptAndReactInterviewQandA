import React, { useState } from 'react';

export default function App() {
  const str = [234, 234, 2, 3, 45, 5, 6];

  const min = str.reduce((acc, cur) => {
    return acc < cur ? acc : cur;
  });

  console.log(min);

  const max = str.reduce((acc, cur) => {
    return acc > cur ? acc : cur;
  });

  console.log(max);

  return <div></div>;
}
