
export default function App() {
  const arr = [8, 19, 5, 6, 14, 9, 13];
  const odds = [];
  const even = [];
  arr.forEach((num) => {
    if (num % 2 === 1) {
      odds.push(num);
    } else {
      even.push(num);
    }
  });

  console.log(odds);
  console.log(even);
  return (
    <div className="App">
      <h1>oddAndEven</h1>
    </div>
  );
}
