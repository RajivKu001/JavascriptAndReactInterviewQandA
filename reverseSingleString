export default function App() {
  const str = "Rajiv Kumar";
  const strArr = str.split(" ");

  const rsSingleStr = strArr.map((el) => {
    return [...el].reverse().join("");
  });

  console.log(rsSingleStr.join(" "));

  return (
    <div className="App">
      <h1>reverseSingleValue</h1>
    </div>
  );
}
