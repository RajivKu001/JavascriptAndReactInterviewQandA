export default function App() {
  const str = "Rajiv Kumar";
  const strArr = str.split(" ");
  console.log(strArr);

  const rsSingleStr = strArr
    .map((el) => {
      return [...el].reverse();
    })
    .map((el) => {
      return el.join("");
    });

  console.log(rsSingleStr.join(" "));

// vijaR ramuK

  return (
    <div className="App">
      <h1>reverseSingleValue</h1>
    </div>
  );
}
