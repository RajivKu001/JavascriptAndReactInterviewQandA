  const arr = 100;

  function myFN(arr) {
    let str = [];
    for (let i = 0; i <= arr; i++) {
      if (i % 3 === 0 && i % 5 === 0) {
        str.push("fizzbuzz");
      } else if (i % 3 === 0) {
        str.push("fizz");
      } else if (i % 5 === 0) {
        str.push("buzz");
      } else {
        str.push(i);
      }
    }
    return str.splice(1);
  }

  console.log(myFN(arr));

