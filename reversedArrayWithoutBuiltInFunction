
export default function App() {
  const fruits = ["Banana", "Orange", "Lemon", "Apple", "Mango"];
  const reversedArray = [];

  for (let i = fruits.length - 1; i >= 0; i--) {
    reversedArray.push(fruits[i]);
  }

  console.log(reversedArray);

  return (
    <div className="App">
      <h1>reversedArrayWithoutBuiltInFunction</h1>
    </div>
  );
}
