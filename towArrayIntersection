  const arr = [1,2,3,4,5];
  const arr1 = [3,4,5,6,7];

  function abc (arr1,arr2) {
    const arr = [];
    for(let i = 0; i < arr1.length; i++){
      for (let j = 0; j <arr2.length; j++){
        if(arr1[i] === arr2[j]){
          arr.push(arr1[i])
        }
      }
    }
    return arr;
  }
  console.log(abc(arr,arr1));
